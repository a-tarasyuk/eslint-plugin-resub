{
  "name": "eslint-plugin-resub",
  "version": "0.5.1",
  "description": "ESlint rules for ReSub",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin-resub",
    "resub"
  ],
  "author": {
    "name": "Alexander Tarasyuk",
    "email": "alexander.tarasyuk@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/a-tarasyuk/eslint-plugin-resub.git"
  },
  "bugs": {
    "url": "https://github.com/a-tarasyuk/eslint-plugin-resub/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "format": "prettier --write \"./**/*.{ts,md}\"",
    "format:check": "prettier --list-different \"./**/*.{ts,md}\"",
    "test": "jest",
    "lint": "eslint --config .eslintrc --ext .ts src tests",
    "build": "tsc"
  },
  "devDependencies": {
    "@types/eslint": "6.1.3",
    "@types/node": "12.12.17",
    "@typescript-eslint/eslint-plugin": "2.11.0",
    "@typescript-eslint/parser": "2.11.0",
    "eslint": "6.7.2",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "prettier": "1.19.1",
    "ts-jest": "24.2.0",
    "typescript": "3.7.3"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "2.11.0"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^2.11.0",
    "eslint": "^6.7.2"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test && npm run build"
    }
  },
  "license": "MIT"
}
