{
  "name": "eslint-plugin-resub",
  "version": "0.5.3",
  "description": "ESlint rules for ReSub",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin-resub",
    "resub"
  ],
  "author": {
    "name": "Alexander Tarasyuk",
    "email": "alexander.tarasyuk@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/a-tarasyuk/eslint-plugin-resub.git"
  },
  "bugs": {
    "url": "https://github.com/a-tarasyuk/eslint-plugin-resub/issues"
  },
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "format": "prettier --write \"./**/*.{ts,md}\"",
    "format:check": "prettier --list-different \"./**/*.{ts,md}\"",
    "test": "jest",
    "lint": "eslint --config .eslintrc --ext .ts src tests",
    "build": "tsc"
  },
  "devDependencies": {
    "@types/eslint": "7.2.2",
    "@types/node": "14.6.4",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "2.11.0",
    "eslint": "6.7.2",
    "husky": "4.2.5",
    "jest": "26.4.2",
    "prettier": "2.1.1",
    "ts-jest": "26.3.0",
    "typescript": "4.0.2"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "4.0.1"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.1"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test && npm run build"
    }
  },
  "license": "MIT"
}
